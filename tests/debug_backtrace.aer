class Main {

	function __construct() {
		$this->b($this->a('First A'), $this->a('Second A'), $this->a('Third A'));
	}

	function a($p) {
		$backtrace = debug_backtrace();
		if(isset($backtrace[0]['args'])) {
			var_export($backtrace[0]['args']);
		} else {
			print("Cannot aquire arguments\n");
		}
		return $p;
	}

	function b($p1, $p2, $p3) {
		print("$p1, $p2, $p3");
	}

}

new Main();
