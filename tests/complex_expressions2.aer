class Program {

	private void test(int $a = (int(int $a, int $b, int $c){return $a+$b+$c;})(14, 10+2, 15), int $b = 0, int $c = 98) {
		print($a + PHP_EOL);
		print($b + PHP_EOL);
		print($c + PHP_EOL);
	}

	public void main() {
		$this->test();
		$this->test(512);
		$this->test(1024, 32);
		$this->test(1000, 2000, 4000);
	}

}
